{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --configuration development",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "check-format": "npm run prettier && npm run lint",
    "e2e": "ng e2e",
    "generate": "nswag swagger2tsclient /runtime:NetCore20 /input:http://localhost:5000/swagger/doc/swagger.json /output:./src/app/core/client/api.client.ts /Template:Angular /ClientBaseClass:BaseClient /ExtensionCode:\"import { BaseClient } from './api.client.base'\" /BaseUrlTokenName:\"API_BASE_URL\" /UseTransformOptionsMethod:true /UseTransformResultMethod:true /InjectionTokenType:InjectionToken"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "auth0-js": "^9.10.1",
    "core-js": "^2.5.6",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@types/file-saver": "^2.0.2",
    "codelyzer": "^6.0.1",
    "node-sass": "^4.14.1",
    "nswag": "^13.10.8",
    "prettier": "^2.2.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7"
  }
}
