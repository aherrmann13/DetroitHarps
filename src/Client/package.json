{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --configuration development",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "check-format": "npm run prettier && npm run lint",
    "e2e": "ng e2e",
    "generate": "nswag swagger2tsclient /runtime:NetCore20 /input:http://localhost:5000/swagger/doc/swagger.json /output:./src/app/core/client/api.client.ts /Template:Angular /ClientBaseClass:BaseClient /ExtensionCode:\"import { BaseClient } from './api.client.base'\" /BaseUrlTokenName:\"API_BASE_URL\" /UseTransformOptionsMethod:true /UseTransformResultMethod:true /InjectionTokenType:InjectionToken"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@types/file-saver": "^2.0.0",
    "auth0-js": "^9.10.1",
    "codelyzer": "^4.3.0",
    "core-js": "^2.5.6",
    "file-saver": "^2.0.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.1.0",
    "tslint": "^5.10.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "node-sass": "^4.14.1",
    "nswag": "^13.10.8",
    "prettier": "^1.17.1",
    "typescript": "^3.1.8"
  }
}
